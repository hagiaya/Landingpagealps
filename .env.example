# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# OpenAI API (for Vercel AI SDK)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API (optional, for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# WhatsApp Integration
# Pilih provider: 'whatsapp-business', 'twilio', 'fontte', atau 'generic'
WHATSAPP_PROVIDER=fontte  # atau 'whatsapp-business', 'twilio', 'generic'

# Konfigurasi untuk WhatsApp Business API (Meta)
WHATSAPP_BUSINESS_ACCESS_TOKEN=your_whatsapp_business_access_token_here
WHATSAPP_BUSINESS_PHONE_NUMBER_ID=your_whatsapp_business_phone_number_id_here

# Konfigurasi untuk Twilio WhatsApp API
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_WHATSAPP_NUMBER=whatsapp:+your_twilio_whatsapp_number_here

